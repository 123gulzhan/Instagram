@model System.Collections.Generic.List<Instagram.Models.User>

@{
    ViewBag.Title = Model.Count() != 0 ? "Результаты поиска" : "Поиск не дал результатов";
    Layout = "_Layout";
}

<h4>@ViewBag.Title</h4>
@if (Model.Count() != 0)
{
    <div>
        <ul>
            @foreach (var user in Model)
            {
                <li><a asp-action="UserProfile" asp-route-id="@user.Id">@user.UserName</a></li>
            }
        </ul>
    </div>
}
